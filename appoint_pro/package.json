{
  "name": "appoint_pro",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "debug:client": "NEXT_PUBLIC_DEBUG=true next dev",
    "debug:server": "cross-env NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "debug:full": "cross-env NODE_OPTIONS='--inspect=0.0.0.0:9229' NEXT_PUBLIC_DEBUG=true next dev",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed-plans": "tsx -r tsconfig-paths/register src/scripts/seed-stripe-plans-script.ts",
    "db:status": "prisma db pull --print",
    "db:setup": "npm run db:deploy && npm run db:seed && npm run db:seed-plans",
    "db:setup-supabase": "tsx -r tsconfig-paths/register src/scripts/setup-supabase.ts",
    "supabase:check": "npm run db:setup-supabase",
    "stripe:sync": "tsx -r tsconfig-paths/register src/scripts/sync-subscriptions.ts",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test \"npm run dev\" http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test \"npm run dev\" http://localhost:3000 \"cypress run --e2e\"",
    "postinstall": "npm run db:deploy",
    "email": "email dev"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.12",
