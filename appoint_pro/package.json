{
  "name": "appoint_pro",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed-plans": "tsx src/scripts/seed-stripe-plans.js",
    "db:status": "prisma db pull --print",
    "db:setup": "npm run db:deploy && npm run db:seed && npm run db:seed-plans",
    "stripe:sync": "tsx -r tsconfig-paths/register src/scripts/sync-subscriptions.ts",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test \"npm run dev\" http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test \"npm run dev\" http://localhost:3000 \"cypress run --e2e\""
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
